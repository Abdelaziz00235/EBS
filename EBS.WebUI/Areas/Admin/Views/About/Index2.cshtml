@model List<ResultAboutDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@{
    int count = 0;
}
<table>
    <tr>#</tr>
  
     <th> ImageUrl1 </th>
     <th> ImageUrl2 </th>
     <th> description </th>
     <th> Item1 </th>
     <th> Item2 </th>
     <th> Item3 </th>
     <th> Item4 </th>
     <th> IsActived </th>
     <th> CreatedAt </th>
     <th> UpdatedAt </th>
     <th> Actions </th>
    @foreach(var item in Model)
    {
        count++;
        <tr>
            <th>@count</th>
            <th><img src="@item.ImageUrl1" style="height:75px;width:75px" /></th>
            <th><img src="@item.ImageUrl2" style="height:75px;width:75px" /></th>
            <th>@item.description</th>
            <th>@item.Item1</th>
            <th>@item.Item2</th>
            <th>@item.Item3</th>
            <th>@item.Item4</th>
            <th>@item.IsActived</th> 
            <td>
                <a asp-action="UpdateAbout" asp-controller="About" asp-area="Admin" class="btn btn-outline-success">Update</a>
                <a asp-action="UpdateAbout" asp-controller="About" asp-area="Admin" class="btn btn-outline-primary">Detail</a>
                <a asp-action="UpdateAbout" asp-controller="About" asp-area="Admin" class="btn btn-outline-danger">Delete</a>
            </td>
        </tr>
    }

</table>
<br />
<a asp-action="CreateAbout" asp-controller="About" asp-area="Admin" class="btn btn-outline-primary">Nouveau</a>